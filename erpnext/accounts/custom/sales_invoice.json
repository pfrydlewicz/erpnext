{
 "custom_fields": [
    {
      "fieldname": "is_partial_invoice",
      "insert_after": "is_return", 
      "description": "Partial invoicing",
      "fieldtype": "Check",
      "label": "Is Partial Invoice",
      "default": 0,
      "dt": "Sales Invoice"
    },
    {
      "fieldname": "partial_imvoice_section",
      "insert_after": "due_date", 
